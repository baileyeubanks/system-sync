<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>HOME</key>
		<string>/Users/_mxappservice</string>
		<key>PATH</key>
		<string>/usr/local/bin:/opt/homebrew/bin:/usr/bin:/bin</string>
	</dict>
	<key>Label</key>
	<string>com.blaze.email-triage-urgent</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>/Users/_mxappservice/ACS_CC_AUTOBOT/blaze-v4/ops/scripts/email_triage_urgent.sh</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/Users/_mxappservice/blaze-logs/email-triage-urgent.err.log</string>
	<key>StandardOutPath</key>
	<string>/Users/_mxappservice/blaze-logs/email-triage-urgent.log</string>
	<key>StartInterval</key>
	<integer>1800</integer>
	<key>WorkingDirectory</key>
	<string>/Users/_mxappservice/ACS_CC_AUTOBOT/blaze-v4/ops/scripts</string>
</dict>
</plist>
