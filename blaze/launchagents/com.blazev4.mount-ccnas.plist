<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.blazev4.mount-ccnas</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-lc</string>
    <string>cd /Users/_mxappservice/Blaze-V4 &amp;&amp; bash ops/scripts/mount_cc_nas.sh &gt;&gt; /Users/_mxappservice/Blaze-V4/logs/mount-ccnas.log 2&gt;&amp;1</string>
  </array>

  <key>RunAtLoad</key>
  <true/>

  <key>StartInterval</key>
  <integer>300</integer>

  <key>StandardOutPath</key>
  <string>/Users/_mxappservice/Blaze-V4/logs/launchd-mount-ccnas.out.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/_mxappservice/Blaze-V4/logs/launchd-mount-ccnas.err.log</string>
</dict>
</plist>
