<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.blaze.log-rotation</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>/Users/_mxappservice/ACS_CC_AUTOBOT/blaze-v4/ops/scripts/rotate_logs.sh</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>23</integer>
		<key>Minute</key>
		<integer>55</integer>
	</dict>
	<key>StandardOutPath</key>
	<string>/Users/_mxappservice/blaze-logs/log-rotation.log</string>
	<key>StandardErrorPath</key>
	<string>/Users/_mxappservice/blaze-logs/log-rotation.err.log</string>
	<key>EnvironmentVariables</key>
	<dict>
		<key>HOME</key>
		<string>/Users/_mxappservice</string>
	</dict>
</dict>
</plist>
